"VertexLitGeneric"
{
	"$basetexture"	"models\weapons\w_dynamite\w_dynamite"
	"$bumpmap" "models/weapons\w_dynamite\w_dynamite_normal"

	"$phong" "1"
//	"$phongexponenttexture" "models\weapons\w_dynamite\w_dynamite_exponent"
	"$phongexponent" "1"
//	"$phongboost" "0.35"	
	"$lightwarptexture" "models\weapons\w_dynamite\w_dynamite_lightwarp"
	"$phongfresnelranges"	"[2 5 10]"
	"$halflambert" "1"
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}