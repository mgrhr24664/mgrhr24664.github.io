"VertexLitGeneric"
{
	$baseTexture "models\weapons\v_cod_knife\Ballistic_Knife"
	$bumpmap "models\weapons\v_cod_knife\Ballistic_Knife_Normals"
	
	"$phongexponent" "255"
	"$phongboost" "5"	
	
	
	"$lightwarptexture" "models\weapons\c_items/c_ambassador_lightwarp"
			
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[.022 .022 .022]"

	"$tempvar" "0"

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "0.5"						
	"$rimlightexponent" "2"				
	"$rimlightboost" "0.5"
	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}
